    result.append(x)
assert result == [1, 3, 5], 'continue skips iteration'

# === Continue with else ===
flag = 0
for x in [1, 2, 3]:
    if x == 2:
        continue
else:
    flag = 1
assert flag == 1, 'continue does not skip else clause'

