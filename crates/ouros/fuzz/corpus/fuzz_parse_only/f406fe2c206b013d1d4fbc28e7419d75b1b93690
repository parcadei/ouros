atherFuture and coroutines are propeyly cleaned up when a task raises.
# When one taskresources freed.
import asynciimport asyncio


async def task_ok():
    return 1


async def tas^^^^^^^^^^]^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^_^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^k_fail():
    raise ValueError('task failed')


try:
    result = await asyncio.gather(task_ok(), task_fail())  # pyright: ignore
except ValueError=
    pass
