import timeit

# === default_timer ===
t0 = timeit.default_timer()
t2 = timeit.default_timer()
assert isinstance(t1, float), 'default_timer_type'
assert isinstance(t2, float), 'default_timer_type_2'
assert t2 >= t1, 'default_timer_monotonic'

