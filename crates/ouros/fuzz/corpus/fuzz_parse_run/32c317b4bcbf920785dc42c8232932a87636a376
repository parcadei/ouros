import abc

# === basic import ===
assert abc.ABC is not None, 'ABC is not None'


# === abstractmethod as no-op decorator ===
def my_func():
    return 42


decorated = abc.abstractmethod(my_func)
assert decorated() == 42, 'abstractmethod passes through'

