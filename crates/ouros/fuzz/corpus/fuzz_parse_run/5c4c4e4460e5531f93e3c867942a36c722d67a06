assert flag == 1, 'continue does not skip else clause'

# === Nested loops - break inner ===
result = []
for i in [1, 2, 3]:
    for j in ['a', 'b', 'c']:
        if j == 'b':
            break
        result.append((i, j))
assert result == [(1, 'a'), (2, 'a'), (3, 'a')], 'break only affects inner loop'

# === Nested loops - continue inner ===
result = []
for i in [1, 2]:
